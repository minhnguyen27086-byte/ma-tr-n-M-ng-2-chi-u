
#include <iostream>
using namespace std;
#define MAX 16

void Nhapmang(int a[][MAX], int &n);
void Xuatmang(int a[][MAX], int n);

long long fib(int k) {
    long long f0 = 0, f1 = 1;
    if (k == 0) return 0;
    if (k == 1) return 1;

    long long fn;
    for (int i = 2; i <= k; i++) {
        fn = f0 + f1;
        f0 = f1;
        f1 = fn;
    }
    return fn;
}
void Nhapmang(int a[][MAX], int &n){
    cin >> n;

for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            a[i][j] = fib(i + j + 1);
        }
    }
}
void Xuatmang(int a[][MAX], int n){
 for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cout << a[i][j] << " ";
        }
        cout << endl;
    }
}

int main()
{
    int n, a[MAX][MAX];
	Nhapmang(a,n);
	Xuatmang(a,n);
	
    return 0;
}
