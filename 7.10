#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int a[10000];
    for (int i = 0; i < n; i++) cin >> a[i];

    for (int i = 0; i < n / 2; i++) {
        if (a[i] != a[n - 1 - i]) {
            cout << "array is not palindrome";
            return 0;
        }
    }

    cout << "array is palindrome";
    return 0;
}
