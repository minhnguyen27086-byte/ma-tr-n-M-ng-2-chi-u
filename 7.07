#include <iostream>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;

    // Ma trận phải vuông
    if (n != m) {
        cout << "No";
        return 0;
    }

    double A[100][100];

    // Nhập ma trận
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cin >> A[i][j];
        }
    }

    // Kiểm tra đối xứng
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (A[i][j] != A[j][i]) {
                cout << "No";
                return 0;
            }
        }
    }

    cout << "Yes";
    return 0;
}
